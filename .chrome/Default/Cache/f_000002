    }
    DynamicExpressionBindingBehavior.prototype.bind = function (binding, source, rawExpression) {
        // Parse the expression that was passed as a string argument to
        // the binding behavior.
        var expression = this.parser.parse(rawExpression);
        // Rebase the expression
        expression = Object(_expression_rebaser__WEBPACK_IMPORTED_MODULE_2__["rebaseExpression"])(expression, binding.sourceExpression);
